<section id="gigs" class="container">
  <div class="form-wrap">
    <h1>All Cars</h1>
    {{#each cars}}
      <div class="car">
      <h3>{{year}} {{make}} {{model}}</3>
      <div>
        <small>Mileage: <span>{{mileage}}</span></small>
      </div>
      <div>
        <small>Contact Email: <span>{{contact_email}}</span></small>
      </div>
    </div>
    {{else}}
      <p>No cars in site</p>
    {{/each}}    
  </div>
    


    
  </section>